type Query {
    listDogs(filter: ListDogsFilter!): ListDogsResponse
    findMatchingDogs(request: FindMatchingDogsInput): [Dog!]
}

type Dog {
    name: String!
    age: Int!
}

input ListDogsFilter {
    offset: Int!
    limit: Int!
}

type ListDogsResponse {
    content: [Dog!]
    hasMore: Boolean
}

input FindMatchingDogsInput {
    homeType: String
    timeAtHome: String
}