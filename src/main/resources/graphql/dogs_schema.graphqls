type Query {
    listDogs(filter: ListDogsFilter!): ListDogsResponse
    findMatchingDogs(request: FindMatchingDogsRequest): [Dog!]
}

type Dog {
    name: String!
    age: Int!
}

input ListDogsFilter {
    offset: Int!
    limit: Int!
}

type ListDogsResponse {
    content: [Dog!]
    hasMore: Boolean
}

input FindMatchingDogsRequest {
    question: String!
}